{
    "name": "sauto-tecnologia/laravel-boleto",
    "type": "library",
    "description": "Biblioteca com boletos para o laravel",
    "keywords": [
        "sauto-tecnologia",
        "laravel-boleto"
    ],
    "homepage": "https://github.com/sauto-tecnologia/laravel-boleto",
    "license": "MIT",
    "authors": [
        {
            "name": "Sauto Tecnologia",
            "email": "suporte@sauto.com.br"
        }
    ],
    "require": {
        "php" : ">=5.5.0",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "nesbot/carbon": "*",
        "illuminate/contracts": "~5.1",
        "illuminate/support": "~5.1",
        "illuminate/view": "~5.1",
        "illuminate/events": "~5.1",
        "neitanod/forceutf8": "^2.0",
        "setasign/fpdf": "1.84"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0.0",
        "orchestra/testbench": "3.2"
    },
    "autoload": {
        "psr-4": {
            "Eduardokum\\LaravelBoleto\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Eduardokum\\LaravelBoleto\\Tests\\": "tests"
        }
